{
   "name": "autumnblaze",
   "version": "0.18.3",
   "description": "another general purpose discord bot",
   "main": "build/bot.js",
   "private": true,
   "scripts": {
      "buildandrun": "npm run build && npm run start",
      "build": "ncc build src/index.ts -o build -m",
      "build:dev": "ncc build src/index.ts -o build -m -s",
      "build:tsc": "tsc",
      "build:dev:tsc": "tsc --project tsconfig.dev.json",
      "buildandrun:tsc": "npm run build:tsc && npm run start",
      "buildandrun:dev:tsc": "npm run build:dev:tsc && npm run start",
      "lint": "eslint --ext .ts src/",
      "test": "echo im lazy lol",
      "docs": "typedoc --out docs src",
      "start": "node build",
      "start:js": "node js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/PCelestia/autumnblaze.git"
   },
   "keywords": [
      "bot",
      "javascript",
      "discordjs",
      "discord-js",
      "mongodb",
      "nodejs",
      "equestria",
      "loveandtolerance",
      "pony",
      "js"
   ],
   "author": "PCelestia",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/PCelestia/autumnblaze/issues"
   },
   "engines": {
      "node": ">14.0.0"
   },
   "homepage": "https://github.com/PCelestia/autumnblaze#readme",
   "dependencies": {
      "@discordjs/opus": "^0.3.2",
      "@shelf/fast-chunk-string": "^1.1.1",
      "bufferutil": "^4.0.1",
      "discord.js": "^12.4.0",
      "erlpack": "^0.1.3",
      "ffmpeg-static": "^4.2.7",
      "html-escaper": "^3.0.0",
      "json2typescript": "^1.4.1",
      "mongodb": "^3.6.2",
      "nedb": "^1.8.0",
      "superagent": "^5.3.1",
      "utf-8-validate": "^5.0.2",
      "winston": "^3.3.3",
      "youtube-dl": "^3.0.2",
      "zlib-sync": "^0.1.7"
   },
   "devDependencies": {
      "@types/html-escaper": "^3.0.0",
      "@types/mongodb": "^3.5.31",
      "@types/node": "^14.14.2",
      "@types/superagent": "^4.1.10",
      "@types/ws": "^7.2.7",
      "@types/youtube-dl": "^1.12.2",
      "@typescript-eslint/eslint-plugin": "^4.5.0",
      "@typescript-eslint/parser": "^4.5.0",
      "@vercel/ncc": "^0.24.1",
      "console-stamp": "^0.2.9",
      "cross-env": "^7.0.2",
      "dotenv": "^8.2.0",
      "eslint": "^7.11.0",
      "ts-prune": "^0.8.4",
      "typedoc": "^0.19.2",
      "typescript": "^4.0.3"
   }
}
